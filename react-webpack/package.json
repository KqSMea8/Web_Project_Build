{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "react-webpack environment",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/.bin/standard",
    "fix": "node_modules/.bin/standard --fix",
    "dev": "cross-env NODE_ENV=development PORT=4200 node build/webpack.dev.config.js",
    "build": "cross-env NODE_ENV=production node_modules/.bin/webpack --progress --colors --config ./build/webpack.production.config.js",
    "precommit": "lint-staged"
  },
  "author": "Mark_Zhang",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jsx-loader": "^0.13.2",
    "lint-staged": "^7.0.5",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.4",
    "react-hot-loader": "^4.1.2",
    "react-loadable": "^5.4.0",
    "react-router-redux": "^5.0.0-alpha.9",
    "sass-loader": "^7.0.1",
    "standard": "^11.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.4"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/dist/",
      "yarn.lock"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "standard --fix",
      "git add"
    ]
  }
}
