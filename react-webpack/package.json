    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.47",
    "@babel/node": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "@babel/preset-react": "^7.0.0-beta.47",
    "@babel/preset-stage-0": "^7.0.0-beta.47",
    "autoprefixer": "^8.5.0",
    "babel-core": "^7.0.0-beta.47",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.5",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jsx-loader": "^0.13.2",
    "lint-staged": "^7.1.2",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "react-hot-loader": "^4.2.0",
    "react-loadable": "^5.4.0",
    "react-router-redux": "^5.0.0-alpha.9",
    "sass-loader": "^7.0.1",
    "standard": "^11.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-bundle-analyzer": "^2.13.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-stylish": "^0.1.8"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/dist/",
      "yarn.lock"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "standard --fix",
      "git add"
    ]
  },
  "scripts": {
    "test": "node_modules/.bin/standard",
    "fix": "node_modules/.bin/standard --fix",
    "dev": "cross-env NODE_ENV=development PORT=4200 node build/webpack.config.dev.babel.js",
    "build": "cross-env NODE_ENV=production ASSET_PATH='./' node_modules/.bin/webpack --progress --colors --config ./build/webpack.config.production.babel.js",
    "build:online": "cross-env NODE_ENV=production ASSET_PATH='http://you_own_cdn_link' node_modules/.bin/webpack --progress --colors --config ./build/webpack.config.production.babel.js",
    "precommit": "lint-staged"
  }
}
